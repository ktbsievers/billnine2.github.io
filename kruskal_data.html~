PImage motsShowing = [];
PImage stabs = [];

float slider_width = 20;
float slider_height = 60;
float posx, posy;
boolean over = false;
boolean locked = false;
float xoff;
float fundo;
int tmotsindex;
float time;
float lineposx;

void setup()
{//Code in setup() only runs once
    	size(1880,900); 
	for (i=0 ; i<170 ; i++) {
	motsShowing[i] = loadImage("Images_and_Figures/toroidal/motsv1/"+str(145+i*5)+".png");
	}
	for (i=0 ; i<10 ; i++) {
	stabs[i] = loadImage("Images_and_Figures/toroidal_stab/lambda_"+str(i)+".png");
	}
	posx = width/2;
  	posy = height*7/8;
  	rectMode(CENTER);
	time = int((posx-740)/2)*.005;
	lineposx = 1600;
}



void draw()
{//Code in draw() will keep running
	background(200,200,200);
	fill(0,0,0);	
	textSize(40);
	text("T = "+str(time),width/2-50,float(height)*6.5/8.);

	tmotsindex = int((posx-740)/2);
	
	textSize(20);
	text("lambda0 = "+lambda0[str(int(time*1000))],900,150);
	text("lambda1 = "+lambda1[str(int(time*1000))],900,200);
	text("lambda2 = "+lambda2[str(int(time*1000))],900,250);
	text("lambda3 = "+lambda3[str(int(time*1000))],900,300);
	text("lambda4 = "+lambda4[str(int(time*1000))],900,350);
	text("lambda5 = "+lambda5[str(int(time*1000))],900,400);
	text("lambda6 = "+lambda6[str(int(time*1000))],900,450);
	text("lambda7 = "+lambda7[str(int(time*1000))],900,500);
	text("lambda8 = "+lambda8[str(int(time*1000))],900,550);
	text("lambda9 = "+lambda9[str(int(time*1000))],900,600);
	
	if(int(time*1000)>=145 && int(time*1000) < 995){
		image(motsShowing[int((time*1000-145)/5)],20,105);
	}else{text("N/A",140,400)}
	image(stabs[0],1400,10,720/2,476/2);
	image(stabs[1],1400,420/2,720/2,476/2);		
	image(stabs[2],1400,410,720/2,476/2);		
	image(stabs[3],1400,610,720/2,476/2);		
	image(stabs[4],1400,810,720/2,476/2);		
  	stroke(0,200,0);
	line(1443,15,1753,700);
	stroke(0,0,0);
	
	if (dist(mouseX, mouseY, 730,720) < 27) {
   		fill(200);
    		over = true;
  	}
  	else {
    		fill(255);
    		over = false;
  	}
	
	rect(730,720,50,50);

	if (dist(mouseX, mouseY, 790,720) < 27) {
   		fill(200);
    		over = true;
  	}
  	else {
    		fill(255);
    		over = false;
  	}
	
	rect(790,720,50,50);


	fundo = posx;
  	line (730, posy, 1150, posy);

  	if (dist(mouseX, mouseY, posx, posy) < slider_height) {
   		fill(200);
    		over = true;
  	}
  	else {
    		fill(255);
    		over = false;
  	}
	if (posx < 740) {
		posx = 740;
	}
	if (posx > 1140) {
		posx = 1140;
	}
  	rect(posx, posy, slider_width, slider_height);
	if(!locked){
		posx = int((posx-740)/2)*2+740;
	}

}

void mousePressed() {
  	if (over) {
    		locked = true;
    		xoff = mouseX-posx;
  	}
	if (dist(mouseX, mouseY, 790,720) < 27) {
   		posx = posx+ 2;
  	}
	if (dist(mouseX, mouseY, 730,720) < 27) {
   		posx = posx- 2;
	}
}
void mouseDragged() {
  	if (locked) {
    		posx = mouseX-xoff;
  	}
}
void mouseReleased() {
  	locked = false;
	time = int((posx-740)/2)*.005;

}

/*^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^*/
